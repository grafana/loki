{
  "Vendor": true,
  "Deadline": "5m",
  "Concurrency": 2,
  "Linters": {
    "gofmt": {"Command": "gofmt -l -s -w"},
    "goimports": {"Command": "goimports -l -w"}
  },
  "Exclude": [
    "\\.pb\\.go"
  ],

  "Enable": [
    "deadcode",
    "errcheck",
    "goconst",
    "gocyclo",
    "gofmt",
    "goimports",
    "golint",
    "gosec",
    "gosimple",
    "gotype",
    "gotypex",
    "ineffassign",
    "interfacer",
    "megacheck",
    "misspell",
    "nakedret",
    "structcheck",
    "unconvert",
    "unparam",
    "varcheck",
    "vet",
    "vetshadow"
  ]
}
