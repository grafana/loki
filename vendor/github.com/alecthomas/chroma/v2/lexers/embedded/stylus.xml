      </rule>
      <rule pattern="\b(and|if unless|in|is|is a|is defined|is not|isnt|or|not)\b">
        <token type="OperatorWord"/>
      </rule>
      <rule pattern="(\#[a-f0-9]{3,6})">
        <token type="LiteralNumberHex"/>
      </rule>
      <rule pattern="\n">
