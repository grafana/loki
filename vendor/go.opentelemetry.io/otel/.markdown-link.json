{
    "ignorePatterns": [
        {
            "pattern": "^http(s)?://localhost"
        }
    ],
    "replacementPatterns": [
        {
            "pattern": "^/registry",
            "replacement": "https://opentelemetry.io/registry"
        },
        {
            "pattern": "^/docs/",
            "replacement": "https://opentelemetry.io/docs/"
        }
    ],
    "retryOn429": true,
    "retryCount": 5,
    "fallbackRetryDelay": "30s"
}
